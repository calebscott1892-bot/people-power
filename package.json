{
  "name": "people-power",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npm run dev:all",
    "dev:server": "npm --prefix Server run dev",
    "dev:client": "VITE_BACKEND_PORT=\"${C4_BACKEND_PORT:-8787}\" VITE_BACKEND_BASE=\"http://127.0.0.1:${C4_BACKEND_PORT:-8787}\" vite --host 127.0.0.1 --port 5173 --strictPort",
    "dev:all": "concurrently -k --kill-others-on-fail --success first -n server,client -c red,blue \"npm run dev:server\" \"npm run dev:client\"",
    "dev:worker": "wrangler dev --config cloudflare/wrangler.toml --port 8787",
    "dev:all:worker": "concurrently -k --kill-others-on-fail --success first -n worker,client -c magenta,blue \"npm run dev:worker\" \"npm run dev:client\"",
    "bootstrap": "node -e \"const fs=require('fs'); const path=require('path'); const p=process.env.C4_DB_PATH||'server/dev.db'; fs.mkdirSync(path.dirname(p),{recursive:true}); fs.writeFileSync(p,'bootstrapped\\n'); console.log('OK bootstrap '+p);\"",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "start": "node Server/index.js",
    "dev:cf": "wrangler pages dev dist --binding SUPABASE_URL=<your-supabase-url> --binding SUPABASE_ANON_KEY=<your-supabase-anon-key>",
    "deploy:cf": "npm run build && wrangler pages publish dist"
  },
